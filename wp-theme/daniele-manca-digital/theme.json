{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "color": {
      "custom": false,
      "customGradient": false,
      "defaultPalette": false,
      "palette": [
        { "name": "Black", "slug": "black", "color": "#000000" },
        { "name": "White", "slug": "white", "color": "#ffffff" },
        { "name": "Green 500", "slug": "green-500", "color": "#22c55e" },
        { "name": "Blue 500", "slug": "blue-500", "color": "#3b82f6" },
        { "name": "Yellow 500", "slug": "yellow-500", "color": "#eab308" },
        { "name": "Neutral 500", "slug": "neutral-500", "color": "#737373" },
        { "name": "Neutral 600", "slug": "neutral-600", "color": "#525252" },
        { "name": "Neutral 800", "slug": "neutral-800", "color": "#262626" },
        { "name": "Neutral 850", "slug": "neutral-850", "color": "#1f1f1f" },
        { "name": "Neutral 900", "slug": "neutral-900", "color": "#171717" },
        { "name": "Neutral 950", "slug": "neutral-950", "color": "#0a0a0a" }
      ]
    },
    "layout": {
      "contentSize": "1200px",
      "wideSize": "1600px"
    },
    "spacing": {
      "custom": false,
      "spacingScale": { "steps": 8 },
      "spacingSizes": [
        { "name": "XS", "slug": "xs", "size": "0.75rem" },
        { "name": "S", "slug": "s", "size": "1rem" },
        { "name": "M", "slug": "m", "size": "1.5rem" },
        { "name": "L", "slug": "l", "size": "2rem" },
        { "name": "XL", "slug": "xl", "size": "3rem" },
        { "name": "2XL", "slug": "2xl", "size": "4rem" },
        { "name": "3XL", "slug": "3xl", "size": "6rem" }
      ]
    },
    "typography": {
      "customFontSize": false,
      "fontWeights": [
        { "name": "Light", "slug": "light", "fontWeight": "300" },
        { "name": "Regular", "slug": "regular", "fontWeight": "400" },
        { "name": "Medium", "slug": "medium", "fontWeight": "500" },
        { "name": "Semibold", "slug": "semibold", "fontWeight": "600" },
        { "name": "Bold", "slug": "bold", "fontWeight": "700" }
      ],
      "fontFamilies": [
        {
          "fontFamily": "'Inter', sans-serif",
          "name": "Inter",
          "slug": "inter"
        },
        {
          "fontFamily": "'Manrope', sans-serif",
          "name": "Manrope",
          "slug": "manrope"
        }
      ],
      "fontSizes": [
        { "name": "Small", "slug": "small", "size": "0.875rem" },
        { "name": "Base", "slug": "base", "size": "1rem" },
        { "name": "Medium", "slug": "medium", "size": "1.125rem" },
        { "name": "Large", "slug": "large", "size": "1.5rem" },
        { "name": "XL", "slug": "x-large", "size": "2.25rem" },
        { "name": "XXL", "slug": "xx-large", "size": "3rem" },
        { "name": "Display", "slug": "display", "size": "4rem" },
        { "name": "Jumbo", "slug": "jumbo", "size": "5rem" }
      ]
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--black)",
      "text": "var(--wp--preset--color--white)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--inter)",
      "lineHeight": "1.6"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--manrope)",
          "fontSize": "var(--wp--preset--font-size--display)",
          "lineHeight": "1.1"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--manrope)",
          "fontSize": "var(--wp--preset--font-size--xx-large)",
          "lineHeight": "1.2"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--manrope)",
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "lineHeight": "1.3"
        }
      },
      "button": {
        "color": {
          "background": "var(--wp--preset--color--white)",
          "text": "var(--wp--preset--color--black)"
        },
        "border": {
          "radius": "0px"
        },
        "typography": {
          "fontWeight": "600",
          "letterSpacing": "-0.01em"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--white)"
        }
      }
    },
    "blocks": {
      "core/button": {
        "border": {
          "radius": "0px"
        },
        "typography": {
          "fontWeight": "600",
          "letterSpacing": "-0.01em"
        }
      },
      "core/heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--manrope)"
        }
      }
    }
  }
}
